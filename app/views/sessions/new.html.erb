<h1> Login </h1>

<div class="full">
<%= form_tag("/login") do %>
<%= text_field_tag :username, nil, placeholder: 'username or email'%>
<%= password_field_tag :password, nil, placeholder: 'password' %>
<%= submit_tag "Login" %>
<% end %>
 <%= link_to "Log in via Google", "/auth/google_oauth2" %>
</div>

